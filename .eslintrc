{
  "env": {
    "browser": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "import/prefer-default-export": "off",
    "prefer-promise-reject-errors": "off",
    "no-throw-literal": "off",
    "quotes": ["error", "single"],
    "jsx-quotes": ["error", "prefer-double"]
  }
}
